% generate some random numbers and characters
N = 20; % 20 number/character pairs
Offset = 50;
numbers = randn(N,19)
tmptext = char(Offset + randi(70,N,1)) 


% clear the screen to facilitate copy/paste
clc

% html code for header
disp('<html>')
disp('<body>')

% html code for table
disp('<table>')

% loop over elements and print
for ri=1:N
    disp([ '<tr><td> ' num2str(numbers(ri)) 'data </td><td> ' tmptext(ri)...
         ' </td><td>'])
end

% close html
fprintf('<table>\n<body>\n<html>\n')

%%


%% resulting HTML file%%
